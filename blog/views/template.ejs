<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head.ejs') %>
</head>

<body>
    
    <%- include('./partials/header.ejs') %>


    <% switch(page) {   
        case 'sobre': %> 
            <%- include('./pages/about.ejs'); %>
        <% break; case 'servicos': %>
            <%- include('./pages/services.ejs'); %>
        <% break; case 'contacto': %>
            <%- include('./pages/contact.ejs'); %>
        <% break; case 'membros': %>
            <%- include('./pages/team.ejs'); %>
        <% break; case 'blog' : %>
            <%- include('./pages/blog.ejs'); %>
        <% break; case 'blog-detalhe' : %>
            <%- include('./pages/blog-details.ejs'); %>
        <% break; case 'precos' : %>
            <%- include('./pages/prices.ejs'); %>
        <% break; case 'testemunhos' : %>
            <%- include('./pages/testimonial.ejs'); %>
        <% break; case 'orcamento' : %>
            <%- include('./pages/orcamento.ejs'); %>
        <% break; default: %>
                <%- include('./pages/index.ejs'); %>
    <% break; } %>
    

    <!-- Vendor Start -->
    <div class="container-fluid py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container py-5 mb-5">
            <div class="bg-white">
                <div class="owl-carousel vendor-carousel">
                    <% { %>
                    <img src="img/vendor-1.jpg" alt="">
                    <% } %>
                </div>
            </div>
        </div>
    </div>
    <!-- Vendor End -->
    

    <!-- Footer Start -->
    <%- include('./partials/footer.ejs') %>
    <!-- Footer End -->


    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square rounded back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/wow/wow.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/counterup/counterup.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
</body>

</html>