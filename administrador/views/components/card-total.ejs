<!-- Start -->
<% totais ? totais.forEach( (data) => { %>
<div class="col-sm-6 col-xl-3">
    <div class="bg-light rounded d-flex align-items-center justify-content-between p-4">
        <i class="<%= data.icon %> fa-3x text-primary"></i>
        <div class="ms-3">
            <p class="mb-2">Total <%= data.titulo %></p>
            <h6 class="mb-0"> <%= data.total || 0 %> </h6>
        </div>
    </div>
</div>
<% }) : false ; %>
<!-- End -->